# - Config file for the LIBBITCOIN_NODE package
#
#  LIBBITCOIN_NODE_INCLUDE_DIRS - include directories for LIBBITCOIN_NODE
#  LIBBITCOIN_NODE_LIBRARIES    - libraries to link against

get_filename_component(LIBBITCOIN_NODE_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(LIBBITCOIN_NODE_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

include("${LIBBITCOIN_NODE_CMAKE_DIR}/libbitcoin-nodeTargets.cmake")
set(LIBBITCOIN_NODE_LIBRARIES libbitcoin-node)

find_package(libbitcoin-blockchain 3.0.0 REQUIRED)
find_package(libbitcoin-network 3.0.0 REQUIRED)
